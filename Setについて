import java.util.HashSet;
import java.util.Set;

public class Main{
  public static void main(String[] args){
    Set<String> colors = new HashSet<String>();
    colors.add("red");
    colors.add("blue");
    colors.add("Yellow");
    colors.add("red");//equalsで等価と判断される。
    colors.add("brown");
    System.out.println("色は" + colors.size() + "種類");
  }
}

import java.util.HashSet;
import java.util.Set;

public class Main{
  public static void main(String[] args){
    Set<String> colors = new HashSet<String>();
    colors.add("red");
    colors.add("blue");
    colors.add("Yellow");
    colors.add("red");
    colors.add("brown");
    for(String s : colors){
      System.out.print(s + "→");
    }
  }
}
