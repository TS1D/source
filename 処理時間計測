import java.util.*;
public class Main{
  public static void main(String[] args){
    long start = System.currentTimeMillis();
    //ここで何らかの時間がかかる処理
    for(int i = 0; i < 1000000000; i++){
      int x = 1 + i;
      
    }

    long end = System.currentTimeMillis();
    System.out.println("処理にかかった時間はx..." + (end - start) + "ミリ秒でした");

  }
}
