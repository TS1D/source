import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int m = sc.nextInt();

        for(int i = 0; i < n; i++){
            int a = sc.nextInt();
            String format = "%" + m + "d\n";
            System.out.printf(format, a);
        }

        sc.close();
    }
}
